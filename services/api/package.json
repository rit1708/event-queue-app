{
  "name": "queue-api",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "seed": "ts-node-dev --transpile-only scripts/seed.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier -w ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "mongodb": "^7.0.0",
    "socket.io": "^4.8.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
